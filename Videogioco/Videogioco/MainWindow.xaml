<Window x:Class="Videogioco.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Videogioco"
        mc:Ignorable="d"
        Title="Start" Height="450" Width="800">
    <Grid ScrollViewer.HorizontalScrollBarVisibility="Auto">
        <Image HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="800" Source="SFONDO.png" Margin="0,0,-8,-31"/>
        <Button x:Name="btnStart" Content="" HorizontalAlignment="Left" Margin="325,310,0,0" VerticalAlignment="Top" Width="127" Height="41" Click="btnStart_Click">
            <Button.Background>
                <ImageBrush ImageSource="START.png"/>
            </Button.Background>
        </Button>
    </Grid>
</Window>
